{
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 4,
    "printWidth": 120,
    "semi": true,
    "embeddedLanguageFormatting": "auto",
    "endOfLine": "lf",
    "htmlWhitespaceSensitivity": "css",
    "arrowParens": "always",
    "bracketSpacing": true,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "plugins": ["prettier-plugin-tailwindcss"]
}
